{
  "name": "proyectos-test",
  "version": "0.0.0",
  "private": true,
  "productName": "",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack",
    "build-dev-nw": "nw ./src/",
    "build-prod": "nwbuild --platforms win64 --buildDir dist/ ./src/",
    "package-electron": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "build-nexe": "nexe -r */**/*"
  },
  "bin": {
    "execute": "bin/www"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "public/**/*",
      "routes/**/*",
      "views/**/*",
      "db/**/*"
    ],
    "targets": [
      "node10-win-x64"
    ]
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "electron-packager": "^15.4.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-polyfill-webpack-plugin": "^1.1.0",
    "nodemon": "2.0.3",
    "pkg": "^5.3.3",
    "pug": "2.0.0-beta11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.13.13",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "*",
    "css-loader": "^5.2.4",
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1",
    "fs-extra": "^9.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "nexe": "^4.0.0-beta.19",
    "nw": "^0.44.1-sdk",
    "nw-builder": "^3.5.7",
    "packager": "^0.1.9",
    "path-browserify": "^1.0.1",
    "style-loader": "^2.0.0",
    "util": "^0.12.3",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
